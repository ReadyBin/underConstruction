<ResourceDictionary 
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:base="clr-namespace:Vishnu.Interchange;assembly=Vishnu.Interchange"
                    xmlns:attached="clr-namespace:Vishnu.WPF_UI.DependencyProperties">
    <ResourceDictionary x:Key="BitmapImageDictionary">
        <BitmapImage x:Key="None" UriSource="../Media/empty.png" />
        <BitmapImage x:Key="Working" UriSource="../Media/running_man.png" />
        <BitmapImage x:Key="Scheduled" UriSource="../Media/stopwatch.png" />
        <BitmapImage x:Key="EventTriggered" UriSource="../Media/event.png" />
        <BitmapImage x:Key="Done" UriSource="../Media/checkmark_small.png" />
        <BitmapImage x:Key="Waiting" UriSource="../Media/historical.png" />
        <BitmapImage x:Key="Aborted" UriSource="../Media/flash.png" />
        <BitmapImage x:Key="Error" UriSource="../Media/support.png" />
        <BitmapImage x:Key="InternalError" UriSource="../Media/self_distract.png" />
    </ResourceDictionary>
    <SolidColorBrush x:Key="WindowBGBrush" Color="LemonChiffon"/>
    <SolidColorBrush x:Key="WindowBGBrushSleepMode" Color="DeepSkyBlue"/>
</ResourceDictionary>